<?xml version="1.0" ?>

<robot name="my_rb1_robot">

    <!-- Gazebo - colors -->
    <gazebo reference="base_link">
        <material>Gazebo/Orange</material>
    </gazebo>

    <!-- Gazebo - laser -->
    <gazebo reference="joint_front_laser_base_link">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="front_laser">
        <material>Gazebo/Black</material>
    </gazebo>

    <!-- Gazebo - wheels -->
    <gazebo reference="right_wheel">
        <material>Gazebo/Black</material>
        <mu1>0.9</mu1>
        <mu2>0.9</mu2>
    </gazebo>
    <gazebo reference="left_wheel">
        <material>Gazebo/Black</material>
        <mu1>0.9</mu1>
        <mu2>0.9</mu2>
    </gazebo>

    <!-- Gazebo - casters -->
    <gazebo reference="joint_front_caster_base_footprint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="front_caster">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="joint_back_caster_base_footprint">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="back_caster">
        <material>Gazebo/Black</material>
    </gazebo>

    <!-- Gazebo - differential driver -->
    <gazebo>
        <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
            <alwaysOn>true</alwaysOn>
            <updateRate>20</updateRate>
            <leftJoint>joint_left_wheel_base_footprint</leftJoint>
            <rightJoint>joint_right_wheel_base_footprint</rightJoint>
            <wheelSeparation>0.4</wheelSeparation>
            <wheelDiameter>0.05</wheelDiameter>
            <wheelTorque>10</wheelTorque> <!-- Corrected from <torque> -->
            <commandTopic>cmd_vel</commandTopic>
            <odometryTopic>odom</odometryTopic>
            <odometryFrame>odom</odometryFrame>
            <robotBaseFrame>base_footprint</robotBaseFrame>
            <publishWheelTF>true</publishWheelTF>
            <!-- ADDING THESE MISSING PARAMETERS TO FIX WARNING -->
            <rosDebugLevel>info</rosDebugLevel>  <!-- Fixes missing <rosDebugLevel> -->
            <publishOdomTF>true</publishOdomTF>  <!-- Fixes missing <publishOdomTF> -->
            <publishWheelJointState>true</publishWheelJointState>  <!-- Fixes missing <publishWheelJointState> -->
            <wheelAcceleration>0.5</wheelAcceleration>  <!-- Fixes missing <wheelAcceleration> -->
            <publishTf>true</publishTf>  <!-- Fixes missing <publishTf> -->
        </plugin>
    </gazebo>

    <!-- Gazebo - laser sensor -->
    <gazebo reference="front_laser">
        <sensor type="ray" name="head_hokuyo_sensor">
            <pose>0 0 0 0 0 0</pose>
            <visualize>false</visualize>
            <update_rate>20</update_rate>
            <ray>
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-1.570796</min_angle>
                        <max_angle>1.570796</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.20</min>
                    <max>10.0</max>
                    <resolution>0.01</resolution>
                </range>
                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
            <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
                <topicName>/scan</topicName>
                <frameName>front_laser</frameName>
            </plugin>
        </sensor>
    </gazebo>

    <!-- Control plugin -->
    <gazebo>
        <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
            <robotNamespace>/robot</robotNamespace>
            <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
        </plugin>
    </gazebo>


    <!-- Link - base footprint -->
    <link name="base_footprint">
        <origin xyz="0 0 0" rpy="0 0 0" />
    </link>


    <!-- Link - base link -->
    <joint name="joint_base_link_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0.0 0 0.15" />
        <child link="base_link" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="base_link">
        <inertial>
            <mass value="19.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.4509375" ixy="0" ixz="0" iyy="0.4509375" iyz="0" izz="0.609375" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>
    </link>


    <!-- Link - right wheel -->
    <joint name="joint_right_wheel_base_footprint" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.20 -0.015" />
        <child link="right_wheel" />
        <parent link="base_footprint" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="right_wheel">
        <inertial>
            <mass value="1.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000434375" ixy="0" ixz="0" iyy="0.000434375" iyz="0" izz="0.00046875" />
        </inertial>

        <collision>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - left wheel -->
    <joint name="joint_left_wheel_base_footprint" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.20 -0.015" />
        <child link="left_wheel" />
        <parent link="base_footprint" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="left_wheel">
        <inertial>
            <mass value="1.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000434375" ixy="0" ixz="0" iyy="0.000434375" iyz="0" izz="0.00046875" />
        </inertial>

        <collision>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - front caster -->
    <joint name="joint_front_caster_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 -0.015" />
        <child link="front_caster" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025" />
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - back caster -->
    <joint name="joint_back_caster_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 -0.015" />
        <child link="back_caster" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025" />
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - front laser -->
    <joint name="joint_front_laser_base_link" type="fixed">
        <origin rpy="0 0 0" xyz="0.25 0.0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_laser">
        <inertial>
            <mass value="0.5" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.000282292" ixy="0" ixz="0" iyy="0.000282292" iyz="0" izz="0.001225" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.025" length="0.07"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.025" length="0.07"/>
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>

</robot>