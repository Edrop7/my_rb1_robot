<?xml version="1.0" ?>

<robot name="my_rb1_robot">


    <!-- Link - base footprint -->
    <link name="base_footprint">
        <origin xyz="0 0 0" rpy="0 0 0" />
    </link>


    <!-- Link - base link -->
    <joint name="joint_base_link_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0.0 0 0.15" />
        <child link="base_link" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="base_link">
        <inertial>
            <mass value="24.8207" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.5739" ixy="0" ixz="0" iyy="0.5739" iyz="0" izz="0.7756" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0 1 1" />
            </material>
        </visual>
    </link>


    <!-- Link - right wheel -->
    <joint name="joint_right_wheel_base_footprint" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.20 0.015" />
        <child link="right_wheel" />
        <parent link="base_footprint" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="right_wheel">
        <inertial>
            <mass value="0.0331" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.0000" ixy="0" ixz="0" iyy="0.0000" iyz="0" izz="0.0000" />
        </inertial>

        <collision>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - left wheel -->
    <joint name="joint_left_wheel_base_footprint" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.20 0.015" />
        <child link="left_wheel" />
        <parent link="base_footprint" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="left_wheel">
        <inertial>
            <mass value="0.0331" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.0000" ixy="0" ixz="0" iyy="0.0000" iyz="0" izz="0.0000" />
        </inertial>

        <collision>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
        </collision>

        <visual>
            <origin rpy="1.5708 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder radius="0.025" length="0.04"/>
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - front caster -->
    <joint name="joint_front_caster_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 0.015" />
        <child link="front_caster" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_caster">
        <inertial>
            <mass value="0.0276" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.0000" ixy="0" ixz="0" iyy="0.0000" iyz="0" izz="0.0000" />
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - back caster -->
    <joint name="joint_back_caster_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 0.015" />
        <child link="back_caster" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="back_caster">
        <inertial>
            <mass value="0.0276" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.0000" ixy="0" ixz="0" iyy="0.0000" iyz="0" izz="0.0000" />
        </inertial>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>


    <!-- Link - front laser -->
    <joint name="joint_front_laser_base_link" type="fixed">
        <origin rpy="0 0 0" xyz="0.25 0.0 0.075" />
        <child link="front_laser" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <link name="front_laser">
        <inertial>
            <mass value="0.0579" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.0000" ixy="0" ixz="0" iyy="0.0000" iyz="0" izz="0.0001" />
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.025" length="0.07"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.025" length="0.07"/>
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
    </link>

</robot>